<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="syndroid_theme">
        <attr name="primary" format="color" />
        <attr name="primary_dark" format="color" />
        <attr name="primary_light" format="color" />
        <attr name="accent" format="color" />
        <attr name="primary_text" format="color" />
        <attr name="secondary_text" format="color" />
        <attr name="icons" format="color" />
        
        <attr name="connect_btn" format="integer" />
        <attr name="background_image" format="integer" />
        <attr name="favourites_item_bg" format="integer" />
        <attr name="media_play" format="integer" />
        <attr name="media_arrow" format="integer" />
        <attr name="triangle" format="integer" />
    </declare-styleable>
    <declare-styleable name="KeyboardButton">
        <attr name="text" format="string" />
    </declare-styleable>
    <declare-styleable name="StateButton">
        <attr name="display_value" format="string" />
    </declare-styleable>
</resources>
